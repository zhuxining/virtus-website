model LoginLog {
  id        Int      @id @default(autoincrement())
  userId    String
  ip        String
  userAgent String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("login_log")
}

model Announcement {
  id        Int      @id @default(autoincrement())
  content   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("announcement")
}
